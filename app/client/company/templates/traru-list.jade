md-card(md-theme='traru')

  md-toolbar: div(class='md-toolbar-tools')
    span {{vm.title | capitalize}}
    span(flex)
    md-button(ng-if='vm.module.crud.c.status'
      ng-click='vm.addItem($event)' class='md-icon-button'
      aria-label='new client'
    )
      md-icon(md-font-icon='mdi mdi-plus')


  module-list-wrapper(module='vm.module' items='vm.items'
    initialize='{{vm.initialize}}'
  )
    md-list Traru

  md-tabs
    md-tab
      md-tab-label {{ 'MODEL.DEPARTMENT_PLURAL' | translate | capitalize }} 
      md-tab-body aqui los cargos

