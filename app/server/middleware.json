{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!./public/favicons/favicon.ico"
    }
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes:before": {
    "./middleware/x-powered-by": {
      "params": {
        "name": "Traru"
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "paths": "/static",
      "params": "$!./public"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "loopback#errorHandler": {}
  }
}
